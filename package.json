{
  "name": "movies-4all",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --exec sucrase-node ./src/www/server.js",
    "db:create": "sequelize db:create --debug",
    "db:drop": "sequelize db:drop --debug",
    "db:migrate": "sequelize db:migrate --debug",
    "db:seed": "sequelize db:seed:all --debug",
    "db:setup": "npm-run-all db:drop db:create db:migrate db:seed",
    "db:migrate:reset": "sequelize db:migrate:undo:all && npm run db:migrate",
    "docs:generate": "apidoc -f docs.js -o docs/",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.1",
    "apidoc": "^0.17.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "dayjs": "^1.8.15",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^1.6.5",
    "npm-run-all": "^4.1.5",
    "sequelize": "^5.14.0",
    "sequelize-cli": "^5.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-jest": "^24.9.0",
    "cross-env": "^6.0.3",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "sqlite3": "^4.1.1",
    "sucrase": "^3.10.1",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
